    <section id="CTF-write-up">
      <!-- Header -->
      <div id="porto-header">
        <h2>CTF Write Up</h2>
        <p>
          Dive into our comprehensive write-ups, documenting our journey and
          insights from various capture the flag competitions.
        </p>
        <div id="porto-button">
          <a class="showToast">View More</a>
        </div>
      </div>
      <!-- Card Container -->
      <div id="porto-card-container">
        <div id="porto-card" class="col-11 col-md-3">
          <div
            class="card1 custom-rounded box border-1- card1-bg-image mx-2 mx-md-3 mx-lg-4 position-relative"
            style="cursor: pointer;"
          >
            <div class="card1-body text-center">
              <div class="img-container">
                <img
                  src="/ShowcaseProducts.svg"
                  class="custom-rounded box border-1- card1-bg-image img-fluid"
                  style="cursor: pointer;"
                />
              </div>
              <h1
                class="pb-1 fw-bold"
                style="text-align: left; color: white; margin-left: 16px;"
              >
                CarePal
              </h1>
              <h5
                class="pb-1"
                style="text-align: left; color: rgba(255, 255, 255, 0.9); font-size: 16px; margin-bottom: 31px; margin-left: 16px;"
              >
                by dhikarfi
              </h5>
              <div class="frame-container">
                <div class="frame">
                  <p class="caption-left">UI/UX Design</p>
                </div>
                <p class="caption-right">24 Nov 2022</p>
              </div>
              <div
                class="container"
                style="display: flex; justify-content: center;"
